<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">


    <ritman.wallet.utils.OnlyVerticalSwipeRefreshLayout
        android:id="@+id/swipe_refresh"
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <RelativeLayout xmlns:app="http://schemas.android.com/apk/res-auto"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/colorPrimary">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/home_bg_image"
                android:backgroundTint="#332D78" />


            <androidx.core.widget.NestedScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/top_layout"
                android:layout_marginTop="-150dp"
                android:background="@color/white"
                android:clipToPadding="false"
                android:fillViewport="true"
                android:paddingBottom="@dimen/width_100dp"
                android:scrollbars="none">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/backgtound_round_up"
                    android:orientation="vertical"
                    android:paddingTop="50dp">

                    <LinearLayout
                        android:id="@+id/kyc_layouts"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_8sdp"
                        android:layout_marginTop="@dimen/margin_5dp"
                        android:layout_marginEnd="@dimen/_8sdp"
                        android:background="@drawable/border_background"
                        android:orientation="horizontal"
                        android:padding="@dimen/_5sdp"
                        android:visibility="gone"
                        android:weightSum="3.5">

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/_5sdp"
                            android:layout_weight="3"
                            android:paddingStart="10dp"
                            android:text="@string/complete_regi_txt"
                            android:textColor="@color/colorBlack"
                            android:textSize="@dimen/_15sdp" />

                        <ImageView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_weight="0.4"
                            android:padding="@dimen/_8sdp"
                            android:src="@drawable/arrow_menu"
                            app:tint="@color/color_toti" />

                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:layout_marginEnd="@dimen/_5sdp"
                        android:orientation="horizontal"
                        android:weightSum="3">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/create_wallet"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/margin_7dp"
                            android:layout_weight="1"
                            android:background="?attr/selectableItemBackground"
                            android:elevation="@dimen/_5sdp"
                            app:cardCornerRadius="@dimen/_5sdp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@color/colorPrimary"
                                android:gravity="center"
                                android:orientation="vertical"
                                android:padding="@dimen/_5sdp">

                                <ImageView
                                    android:layout_width="30dp"
                                    android:layout_height="25dp"
                                    android:layout_margin="@dimen/_10sdp"
                                    android:adjustViewBounds="true"
                                    android:src="@drawable/ic_mobile_recharge"
                                    app:tint="@color/colorWhite" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/lato_regular"
                                    android:gravity="center"
                                    android:text="@string/create_wallet"
                                    android:textColor="@color/colorWhite"
                                    android:textSize="@dimen/_10sdp" />
                            </LinearLayout>
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/check_rates_card"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/margin_7dp"
                            android:layout_weight="1"
                            android:background="?attr/selectableItemBackground"
                            app:cardCornerRadius="@dimen/_10sdp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@color/colorPrimary"
                                android:gravity="center"
                                android:orientation="vertical"
                                android:padding="@dimen/_5sdp">

                                <ImageView
                                    android:layout_width="30dp"
                                    android:layout_height="25dp"
                                    android:layout_margin="@dimen/_10sdp"
                                    android:adjustViewBounds="true"
                                    android:src="@drawable/ic_exchange"
                                    android:tint="@color/colorWhite" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/lato_regular"
                                    android:gravity="center"
                                    android:text="@string/convert_currency_home"
                                    android:textColor="@color/colorWhite"
                                    android:textSize="@dimen/_10sdp" />
                            </LinearLayout>
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/transactionHistory_card"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/margin_7dp"
                            android:layout_weight="1"
                            android:background="?attr/selectableItemBackground"
                            android:padding="@dimen/_10sdp"
                            app:cardCornerRadius="@dimen/_10sdp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@color/colorPrimary"
                                android:gravity="center"
                                android:orientation="vertical"
                                android:padding="@dimen/_5sdp">

                                <ImageView
                                    android:layout_width="30dp"
                                    android:layout_height="25dp"
                                    android:layout_margin="@dimen/_10sdp"
                                    android:adjustViewBounds="true"
                                    android:src="@drawable/ic_cash_out_payment_icon"
                                    android:tint="@color/colorWhite" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/lato_regular"
                                    android:gravity="center"
                                    android:text="@string/transaction_history_break"
                                    android:textColor="@color/colorWhite"
                                    android:textSize="@dimen/_10sdp" />
                            </LinearLayout>
                        </androidx.cardview.widget.CardView>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:layout_marginEnd="@dimen/_5sdp"
                        android:orientation="horizontal"
                        android:weightSum="3">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/money_transfer"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/margin_7dp"
                            android:layout_weight="1"
                            android:background="?attr/selectableItemBackground"
                            android:padding="@dimen/_10sdp"
                            app:cardCornerRadius="@dimen/_10sdp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@color/colorPrimary"
                                android:gravity="center"
                                android:orientation="vertical"
                                android:padding="@dimen/_5sdp">

                                <ImageView
                                    android:layout_width="30dp"
                                    android:layout_height="25dp"
                                    android:layout_margin="@dimen/_10sdp"
                                    android:adjustViewBounds="true"
                                    android:src="@drawable/ic_money_transfer_icon"
                                    android:tint="@color/colorWhite" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/lato_regular"
                                    android:gravity="center"
                                    android:text="@string/money_transfer"
                                    android:textColor="@color/colorWhite"
                                    android:textSize="@dimen/_10sdp" />
                            </LinearLayout>
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/mobile_top_up_carview"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/margin_7dp"
                            android:layout_weight="1"
                            android:background="?attr/selectableItemBackground"
                            android:elevation="@dimen/_5sdp"
                            android:padding="@dimen/_5sdp"
                            app:cardCornerRadius="@dimen/_5sdp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@color/colorPrimary"
                                android:gravity="center"
                                android:orientation="vertical"
                                android:padding="@dimen/_5sdp">

                                <ImageView
                                    android:layout_width="30dp"
                                    android:layout_height="25dp"
                                    android:layout_margin="@dimen/_10sdp"
                                    android:adjustViewBounds="true"
                                    android:tint="@color/colorWhite"
                                    android:src="@drawable/ic_cash_out_payment_icon"/>

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/lato_regular"
                                    android:gravity="center"
                                    android:text="@string/mobile_top_up_home"
                                    android:textColor="@color/colorWhite"
                                    android:textSize="@dimen/_10sdp" />
                            </LinearLayout>
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/bill_payment_carview"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/margin_7dp"
                            android:layout_weight="1"
                            android:background="?attr/selectableItemBackground"
                            android:padding="@dimen/_10sdp"
                            app:cardCornerRadius="@dimen/_10sdp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@color/colorPrimary"
                                android:gravity="center"
                                android:orientation="vertical"
                                android:padding="@dimen/_5sdp">

                                <ImageView
                                    android:layout_width="30dp"
                                    android:layout_height="25dp"
                                    android:layout_margin="@dimen/_10sdp"
                                    android:adjustViewBounds="true"
                                    android:src="@drawable/bbps_mnemonic_logo"
                                   />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/lato_regular"
                                    android:gravity="center"
                                    android:text="@string/biller_name_home"
                                    android:textColor="@color/colorWhite"
                                    android:textSize="@dimen/_10sdp" />
                            </LinearLayout>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:layout_marginEnd="@dimen/_5sdp"
                        android:orientation="horizontal"
                        android:weightSum="3">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/aeps"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/margin_7dp"
                            android:layout_weight="1"
                            android:background="?attr/selectableItemBackground"
                            android:padding="@dimen/_10sdp"
                            app:cardCornerRadius="@dimen/_10sdp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@color/colorPrimary"
                                android:gravity="center"
                                android:orientation="vertical"
                                android:padding="@dimen/_5sdp">

                                <ImageView
                                    android:layout_width="30dp"
                                    android:layout_height="25dp"
                                    android:layout_margin="@dimen/_10sdp"
                                    android:adjustViewBounds="true"
                                    android:src="@drawable/ic_money_transfer_icon"
                                    app:tint="@color/colorWhite" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/lato_regular"
                                    android:gravity="center"
                                    android:text="@string/aeps"
                                    android:textColor="@color/colorWhite"
                                    android:textSize="@dimen/_10sdp" />
                            </LinearLayout>
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/pl"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/margin_7dp"
                            android:layout_weight="1"
                            android:background="?attr/selectableItemBackground"
                            android:padding="@dimen/_10sdp"
                            app:cardCornerRadius="@dimen/_10sdp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@color/colorPrimary"
                                android:gravity="center"
                                android:orientation="vertical"
                                android:padding="@dimen/_5sdp">

                                <ImageView
                                    android:layout_width="30dp"
                                    android:layout_height="25dp"
                                    android:layout_margin="@dimen/_10sdp"
                                    android:adjustViewBounds="true"
                                    android:src="@drawable/ic_money_transfer_icon"
                                    android:tint="@color/colorWhite" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/lato_regular"
                                    android:gravity="center"
                                    android:text="@string/pl"
                                    android:textColor="@color/colorWhite"
                                    android:textSize="@dimen/_10sdp" />
                            </LinearLayout>
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/pancard"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/margin_7dp"
                            android:layout_weight="1"
                            android:visibility="visible"
                            android:background="?attr/selectableItemBackground"
                            android:padding="@dimen/_10sdp"
                            app:cardCornerRadius="@dimen/_10sdp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@color/colorPrimary"
                                android:gravity="center"
                                android:orientation="vertical"
                                android:padding="@dimen/_5sdp">

                                <ImageView
                                    android:layout_width="30dp"
                                    android:layout_height="25dp"
                                    android:layout_margin="@dimen/_10sdp"
                                    android:adjustViewBounds="true"
                                    android:src="@drawable/ic_cash_out_payment_icon"
                                    android:tint="@color/colorWhite" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/lato_regular"
                                    android:gravity="center"
                                    android:text="@string/pancard"
                                    android:textColor="@color/colorWhite"
                                    android:textSize="@dimen/_10sdp" />
                            </LinearLayout>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_5sdp"
                        android:layout_marginEnd="@dimen/_5sdp"
                        android:orientation="horizontal"
                        android:weightSum="3">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/insurance"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/margin_7dp"
                            android:layout_weight="1"
                            android:background="?attr/selectableItemBackground"
                            android:padding="@dimen/_10sdp"
                            app:cardCornerRadius="@dimen/_10sdp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@color/colorPrimary"
                                android:gravity="center"
                                android:orientation="vertical"
                                android:padding="@dimen/_5sdp">

                                <ImageView
                                    android:layout_width="30dp"
                                    android:layout_height="25dp"
                                    android:layout_margin="@dimen/_10sdp"
                                    android:adjustViewBounds="true"
                                    android:src="@drawable/ic_money_transfer_icon"
                                    android:tint="@color/colorWhite" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/lato_regular"
                                    android:gravity="center"
                                    android:text="@string/insurance"
                                    android:textColor="@color/colorWhite"
                                    android:textSize="@dimen/_10sdp" />
                            </LinearLayout>
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/ekyc"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/margin_7dp"
                            android:layout_weight="1"
                            android:background="?attr/selectableItemBackground"
                            android:padding="@dimen/_10sdp"
                            app:cardCornerRadius="@dimen/_10sdp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@color/colorPrimary"
                                android:gravity="center"
                                android:orientation="vertical"
                                android:padding="@dimen/_5sdp">

                                <ImageView
                                    android:layout_width="30dp"
                                    android:layout_height="25dp"
                                    android:layout_margin="@dimen/_10sdp"
                                    android:adjustViewBounds="true"
                                    android:src="@drawable/ic_cash_out_payment_icon"
                                    android:tint="@color/colorWhite" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/lato_regular"
                                    android:gravity="center"
                                    android:text="@string/eKYC"
                                    android:textColor="@color/colorWhite"
                                    android:textSize="@dimen/_10sdp" />
                            </LinearLayout>
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_margin="@dimen/margin_7dp"
                            android:layout_weight="1"
                            android:visibility="invisible"
                            android:background="?attr/selectableItemBackground"
                            android:padding="@dimen/_10sdp"
                            app:cardCornerRadius="@dimen/_10sdp">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@color/posOrange"
                                android:gravity="center"
                                android:orientation="vertical"
                                android:padding="@dimen/_5sdp">

                                <ImageView
                                    android:layout_width="30dp"
                                    android:layout_height="25dp"
                                    android:layout_margin="@dimen/_10sdp"
                                    android:adjustViewBounds="true"
                                    android:src="@drawable/ic_cash_out_payment_icon"
                                    android:tint="@color/colorWhite" />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/lato_regular"
                                    android:gravity="center"
                                    android:text="@string/pancard"
                                    android:textColor="@color/colorWhite"
                                    android:textSize="@dimen/_10sdp" />
                            </LinearLayout>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/margin_15dp"
                        android:fontFamily="@font/lato_regular"
                        android:text="@string/offer_of_home"
                        android:textColor="@color/black"
                        android:visibility="gone" />

                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/offer_rcv"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="@dimen/_5sdp"
                        android:visibility="gone" />
                </LinearLayout>


            </androidx.core.widget.NestedScrollView>

            <LinearLayout
                android:id="@+id/top_layout"
                android:layout_width="match_parent"
                android:layout_height="300dp"
                android:gravity="start"
                android:orientation="vertical">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:orientation="vertical"
                    android:weightSum="2">


                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginTop="@dimen/margin_20dp"
                        android:layout_weight="1.4">


                        <de.hdodenhof.circleimageview.CircleImageView
                            android:id="@+id/user_image_home"
                            android:layout_width="@dimen/button_height_60dp"
                            android:layout_height="@dimen/button_height_60dp"
                            android:layout_marginStart="@dimen/margin_20dp"
                            android:layout_marginEnd="@dimen/margin_15dp"
                            android:layout_toStartOf="@id/icon"
                            android:src="@drawable/user_profile_home"
                            android:visibility="gone" />


                        <ImageView

                            android:id="@+id/icon"
                            android:layout_width="@dimen/height_150dp"
                            android:layout_height="@dimen/width_100dp"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentBottom="true"
                            android:layout_marginStart="@dimen/margin_20dp"
                            android:foregroundGravity="bottom"
                            android:src="@drawable/home_logo"
                            android:visibility="gone" />

                    </RelativeLayout>


                    <ImageView
                        android:layout_width="150dp"
                        android:layout_height="100dp"
                        android:layout_marginStart="@dimen/margin_20dp"
                        android:src="@drawable/home_logo"
                        android:visibility="visible" />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/lato_regular"
                        android:gravity="center"
                        android:padding="@dimen/text_size_20sp"
                        android:visibility="gone"
                        android:textColor="@color/colorWhite"
                        android:textSize="@dimen/text_size_16sp" />

                    <RelativeLayout
                        android:id="@+id/view_account"
                        android:layout_width="330dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:background="@drawable/user_accounts_box_shadow"
                        android:gravity="center">


                        <RelativeLayout
                            android:id="@+id/balance_layout"
                            android:layout_width="match_parent"
                            android:layout_height="62dp"
                            android:background="?attr/selectableItemBackground">


                            <RelativeLayout
                                android:id="@+id/rel_layout"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">

                                <TextView
                                    android:id="@+id/currencyName"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/montserrat_semibold"
                                    android:letterSpacing="-0.05"
                                    android:layout_marginLeft="20dp"
                                    android:layout_marginRight="20dp"
                                    android:layout_marginTop="@dimen/margin_5dp"
                                    android:text="Indian Rupees (INR)"
                                    android:textColor="@color/colorPrimary"
                                    android:textSize="@dimen/text_size_14sp"
                                    android:visibility="visible" />
                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/margin_10dp"
                                    android:layout_marginBottom="@dimen/margin_10dp"
                                    android:layout_below="@+id/currencyName">
                                    <TextView
                                        android:id="@+id/txt_balance"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="20dp"
                                        android:text="@string/balance_text"
                                        android:visibility="gone"
                                        android:textColor="@color/colorGrayTwo"
                                        android:textSize="@dimen/text_size_16sp" />

                                    <TextView
                                        android:id="@+id/tv_balance"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:text="2000"
                                        android:textStyle="bold"
                                        android:layout_marginLeft="20dp"
                                        android:layout_marginRight="20dp"
                                        android:layout_below="@+id/txt_balance"
                                        android:textColor="@color/colorBlack"
                                        android:textSize="16sp" />
                                </LinearLayout>

                                <ImageView
                                    android:id="@+id/image"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentEnd="true"
                                    android:layout_centerVertical="true" />

                            </RelativeLayout>


                            <de.hdodenhof.circleimageview.CircleImageView
                                android:id="@+id/dummy_image"
                                android:layout_width="@dimen/margin_30dp"
                                android:layout_height="@dimen/margin_30dp"
                                android:layout_centerVertical="true"
                                android:layout_alignParentEnd="true"
                                android:layout_margin="10dp"
                                android:src="@drawable/india" />


                        </RelativeLayout>


                    </RelativeLayout>

                    <ritman.wallet.utils.SnappyRecyclerView
